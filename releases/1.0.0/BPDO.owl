    <!-- https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#Hamilton_depression_rating_scale_17_item_clinical_classification -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#Hamilton_depression_rating_scale_17_item_clinical_classification">
