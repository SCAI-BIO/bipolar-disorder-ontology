        <importedFrom>http://purl.obolibrary.org/obo/DOID_0050700</importedFrom>
        <oboInOwl:hasDefinition>A heart disease and a myopathy that is characterized by deterioration of the function of the heart muscle.</oboInOwl:hasDefinition>
    </owl:Class>
    


    <!-- https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#cardiovascular_system_disease -->

