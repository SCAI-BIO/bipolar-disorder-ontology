    <!-- https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#obstetric_complication -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#obstetric_complication">
        <rdfs:subClassOf rdf:resource="https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#complication"/>
        <rdfs:label>obstetric complication</rdfs:label>
        <Author>Thrishna</Author>
        <fromPubMed></fromPubMed>
        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/pubmed/30181867</oboInOwl:hasDbXRef>
        <oboInOwl:hasDefinition>An obstetric complication is defined as an acute condition arising from a direct cause of maternal death, such as antepartum or postpartum hemorrhage, obstructed labor, postpartum sepsis, complications of abortion, pre-eclampsia or eclampsia, ectopic pregnancy, and ruptured uterus, or indirect causes such as anemia, malaria, and tuberculosis.</oboInOwl:hasDefinition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#obstetric_complication"/>
        <owl:annotatedProperty rdf:resource="https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#oboInOwl:hasDefinition"/>
        <owl:annotatedTarget>An obstetric complication is defined as an acute condition arising from a direct cause of maternal death, such as antepartum or postpartum hemorrhage, obstructed labor, postpartum sepsis, complications of abortion, pre-eclampsia or eclampsia, ectopic pregnancy, and ruptured uterus, or indirect causes such as anemia, malaria, and tuberculosis.</owl:annotatedTarget>
