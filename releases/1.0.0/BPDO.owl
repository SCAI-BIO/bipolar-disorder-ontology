    <!-- https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#chronic_obstructive_pulmonary_disease -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#chronic_obstructive_pulmonary_disease">
