    <!-- https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#memory -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#memory">
        <rdfs:subClassOf rdf:resource="https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#mental_disposition"/>
        <rdfs:label>memory</rdfs:label>
        <CommentOnHierarchy>Followed MFOMD
Refer:https://www.ebi.ac.uk/ols/ontologies/mfomd/terms?iri=http%3A%2F%2Fpurl.obolibrary.org%2Fobo%2FGO_0007613</CommentOnHierarchy>
