    <!-- https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#phospholipase_C_signaling_pathway -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#phospholipase_C_signaling_pathway">
