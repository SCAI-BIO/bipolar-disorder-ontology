        <fromPubMed></fromPubMed>
        <importedFrom>http://purl.obolibrary.org/obo/UBERON_0003881</importedFrom>
        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/pubmed/28115740</oboInOwl:hasDbXRef>
        <oboInOwl:hasDefinition>Part of hippocampus proper bounded by CA2 and the subiculum, characterized by pyramidal neurons that receive projections from pyramidal neurons of CA3 via the Schaffer collaterals.</oboInOwl:hasDefinition>
        <oboInOwl:hasExactSynonym>CA1</oboInOwl:hasExactSynonym>
    </owl:Class>
    


    <!-- https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#CA4_field_of_hippocampus -->

