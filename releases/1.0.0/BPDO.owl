    </owl:Class>
    


    <!-- https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#precuneus_cortex -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/bipolar_disorder#precuneus_cortex">
